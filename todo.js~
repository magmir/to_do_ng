$(document).ready(function() {
	console.log("hi1");
	$('#add').on('click', function (event) {
		event.preventDefault();
		console.log("hi2");
		var todo=$('#toDoWrite').val();
		
		$('.toDoList').prepend('<div class="myList">'
		+ '<div>'
		+ '<input class="check" name="check" type="checkbox"/>'
		+ '</div>'		
		+ '<div class="toDo">'
	 + todo 
		+ '</div>' 
		+ '</div>');
		$('#formToDo')[0].reset();
	});
});


